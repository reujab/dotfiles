{
	"DisableAll": true,
	"Enable": [
		"vet",
		"vetshadow",
		"gotype",
		"deadcode",
		"golint",
		"varcheck",
		"structcheck",
		"errcheck",
		"megacheck",
		"ineffassign",
		"interfacer",
		"unconvert",
		"misspell",
		"unparam"
	],
	"Concurrency": 1,
	"Exclude": [
		"declaration of \"err\" shadows declaration",
		"error return value not checked \\(defer .+\\.Close\\(\\)\\)"
	],
	"Deadline": "5m"
}
